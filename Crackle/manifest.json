{
  "name": "Crackle",
  "version": "1.1.0",
  "description": "Rewrite content pages on 'Crackle' website.",
  "content_scripts": [{
    "matches": [
      "*://*.crackle.com/*",
      "*://*.sonycrackle.com/*"
    ],
    "all_frames": true,
    "run_at": "document_start",
    "js": ["content.js"]
  }],
  "icons": {
     "16": "img/icon16.png",
     "48": "img/icon48.png",
     "55": "img/icon55.png",
     "64": "img/icon64.png"
  },
  "manifest_version": 2
}
